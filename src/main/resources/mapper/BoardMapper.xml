<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="luji.demo.os01.dao.board.BoardMapper">

    <select id="getBoardCategory" resultType="luji.demo.os01.dto.board.BoardCategoryDTO">
        SELECT
            id,
            category,
            type,
            publish,
            createdAt,
            deletedAt
        FROM
            demo.boardCategory
        WHERE
            deletedAt is null
    </select>

</mapper>